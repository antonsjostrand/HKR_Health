      </Button>
      <Button layoutX="42.0" layoutY="552.0" mnemonicParsing="false" onAction="#goBack" text="&lt;&lt;" />
      <TextField fx:id="date" layoutX="87.0" layoutY="182.0" promptText="Ex. 31/12 - 18" />
   </children>
</AnchorPane>
